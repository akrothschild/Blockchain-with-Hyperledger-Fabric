,gBAAgB,EAAE,SAFC;AAGnB,QAAA,oBAAoB,EAAE,SAHH;AAInB,QAAA,eAAe,EAAE;AAJE,OAvBhB;AA6BL,MAAA,YAAY,EAAE,EA7BT;AA8BL,MAAA,eAAe,EAAE;AACf,QAAA,WAAW,EAAE,SADE;AAEf,QAAA,gBAAgB,EAAE,SAFH;AAGf,QAAA,oBAAoB,EAAE,SAHP;AAIf,QAAA,eAAe,EAAE;AAJF,OA9BZ;AAoCL,MAAA,SAAS,EAAE,EApCN;AAqCL,MAAA,WAAW,EAAE;AACX,QAAA,WAAW,EAAE,SADF;AAEX,QAAA,gBAAgB,EAAE,SAFP;AAGX,QAAA,oBAAoB,EAAE,SAHX;AAIX,QAAA,eAAe,EAAE;AAJN,OArCR;AA2CL,MAAA,YAAY,EAAE;AACZ,QAAA,UAAU,EAAE,IADA;AAEZ,QAAA,mBAAmB,EAAE;AAFT;AA3CT,KAAP;AAgDD,GArDY;AAsDP,EAAA,OAtDO,qBAsDG;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBACS,KAAK,CAAC,GAAN,CAAU,wCAAV,CADT;;AAAA;AAAA;AACN,cAAA,IADM,oBACN,IADM;AAGd,cAAA,IAAI,CAAC,OAAL,CAAa,UAAA,CAAA,EAAK;AAChB,oBAAM,IAAG,GAAI,MAAM,CAAC,CAAC,CAAC,IAAH,EAAS,UAAT,CAAN,CAA2B,MAA3B,CAAkC,OAAlC,CAAb;AADgB,oBAGd,QAHc,GASZ,CATY,CAGd,QAHc;AAAA,oBAId,qBAJc,GASZ,CATY,CAId,qBAJc;AAAA,oBAKd,cALc,GASZ,CATY,CAKd,cALc;AAAA,oBAMd,qBANc,GASZ,CATY,CAMd,qBANc;AAAA,oBAOd,SAPc,GASZ,CATY,CAOd,SAPc;AAAA,oBAQd,KARc,GASZ,CATY,CAQd,KARc;;AAWhB,gBAAA,KAAI,CAAC,WAAL,CAAiB,IAAjB,CAAsB;AAAE,kBAAA,IAAI,EAAJ,IAAF;AAAQ,kBAAA,KAAK,EAAE;AAAf,iBAAtB;;AACA,gBAAA,KAAI,CAAC,eAAL,CAAqB,IAArB,CAA0B;AAAE,kBAAA,IAAI,EAAJ,IAAF;AAAQ,kBAAA,KAAK,EAAE;AAAf,iBAA1B;;AACA,gBAAA,KAAI,CAAC,QAAL,CAAc,IAAd,CAAmB;AAAE,kBAAA,IAAI,EAAJ,IAAF;AAAQ,kBAAA,KAAK,EAAE;AAAf,iBAAnB;;AACA,gBAAA,KAAI,CAAC,gBAAL,CAAsB,IAAtB,CAA2B;AAAE,kBAAA,IAAI,EAAJ,IAAF;AAAQ,kBAAA,KAAK,EAAE;AAAf,iBAA3B;;AACA,gBAAA,KAAI,CAAC,YAAL,CAAkB,IAAlB,CAAuB;AAAE,kBAAA,IAAI,EAAJ,IAAF;AAAQ,kBAAA,KAAK,EAAE;AAAf,iBAAvB;;AACA,gBAAA,KAAI,CAAC,SAAL,CAAe,IAAf,CAAoB;AAAE,kBAAA,IAAI,EAAJ,IAAF;AAAQ,kBAAA,KAAK,EAAE;AAAf,iBAApB;AACD,eAjBD;;AAHc;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAqBhB;AA3Ea,CAAf","sourcesContent":["<template>\n  <div class=\"container\">\n    <div class=\"row mt-5\">\n      <div class=\"col\">\n        <h1 class=\"text-center\">COVID-19 DATA</h1>\n      </div>\n    </div>\n    <div class=\"row mt-5\" v-if=\"arrPositive.length > 0\">\n      <div class=\"col\">\n        <h2 class=\"text-center\">Positive</h2>\n        <line-chart\n          :chartData=\"arrPositive\"\n          :options=\"chartOptions\"\n          :chartColors=\"positiveChartColors\"\n          label=\"Positive\"\n        />\n      </div>\n    </div>\n\n    <div class=\"row mt-5\" v-if=\"arrHospitalized.length > 0\">\n      <div class=\"col\">\n        <h2 class=\"text-center\">Hospitalized</h2>\n        <line-chart\n          :chartData=\"arrHospitalized\"\n          :options=\"chartOptions\"\n          :chartColors=\"hospitalizedChartColors\"\n          label=\"Hospitalized\"\n        />\n      </div>\n    </div>\n\n    <div class=\"row mt-5\" v-if=\"arrInIcu.length > 0\">\n      <div class=\"col\">\n        <h2 class=\"text-center\">In ICU</h2>\n        <line-chart\n          :chartData=\"arrInIcu\"\n          :options=\"chartOptions\"\n          :chartColors=\"inIcuColors\"\n          label=\"In ICU\"\n        />\n      </div>\n    </div>\n\n    <div class=\"row mt-5\" v-if=\"arrOnVentilators.length > 0\">\n      <div class=\"col\">\n        <h2 class=\"text-center\">On Ventilators</h2>\n        <line-chart\n          :chartData=\"arrOnVentilators\"\n          :options=\"chartOptions\"\n          :chartColors=\"onVentilatorsColors\"\n          label=\"On Ventilators\"\n        />\n      </div>\n    </div>\n\n    <div class=\"row mt-5\" v-if=\"arrRecovered.length > 0\">\n      <div class=\"col\">\n        <h2 class=\"text-center\">Recovered</h2>\n        <line-chart\n          :chartData=\"arrRecovered\"\n          :options=\"chartOptions\"\n          :chartColors=\"recoveredColors\"\n          label=\"Recovered\"\n        />\n      </div>\n    </div>\n\n    <div class=\"row mt-5 mb-5\">\n      <div class=\"col\">\n        <h2 class=\"text-center\">Deaths</h2>\n        <line-chart\n          v-if=\"arrDeaths.length > 0\"\n          :chartData=\"arrDeaths\"\n          :options=\"chartOptions\"\n          :chartColors=\"deathColors\"\n          label=\"Deaths\"\n        />\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport moment from \"moment\";\n\nimport LineChart from \"./components/LineChart\";\n\nexport default {\n  components: {\n    LineChart\n  },\n  data() {\n    return {\n      arrPositive: [],\n      positiveChartColors: {\n        borderColor: \"#077187\",\n        pointBorderColor: \"#0E1428\",\n        pointBackgroundColor: \"#AFD6AC\",\n        backgroundColor: \"#74A57F\"\n      },\n      arrHospitalized: [],\n      hospitalizedChartColors: {\n        borderColor: \"#251F47\",\n        pointBorderColor: \"#260F26\",\n        pointBackgroundColor: \"#858EAB\",\n        backgroundColor: \"#858EAB\"\n      },\n      arrInIcu: [],\n      inIcuColors: {\n        borderColor: \"#190B28\",\n        pointBorderColor: \"#190B28\",\n        pointBackgroundColor: \"#E55381\",\n        backgroundColor: \"#E55381\"\n      },\n      arrOnVentilators: [],\n      onVentilatorsColors: {\n        borderColor: \"#784F41\",\n        pointBorderColor: \"#784F41\",\n        pointBackgroundColor: \"#BBE5ED\",\n        backgroundColor: \"#BBE5ED\"\n      },\n      arrRecovered: [],\n      recoveredColors: {\n        borderColor: \"#4E5E66\",\n        pointBorderColor: \"#4E5E66\",\n        pointBackgroundColor: \"#31E981\",\n        backgroundColor: \"#31E981\"\n      },\n      arrDeaths: [],\n      deathColors: {\n        borderColor: \"#E06D06\",\n        pointBorderColor: \"#E06D06\",\n        pointBackgroundColor: \"#402A2C\",\n        backgroundColor: \"#402A2C\"\n      },\n      chartOptions: {\n        responsive: true,\n        maintainAspectRatio: false\n      }\n    };\n  },\n  async created() {\n    const { data } = await axios.get(\"https://covidtracking.com/api/us/daily\");\n\n    data.forEach(d => {\n      const date = moment(d.date, \"YYYYMMDD\").format(\"MM/DD\");\n      const {\n        positive,\n        hospitalizedCurrently,\n        inIcuCurrently,\n        onVentilatorCurrently,\n        recovered,\n        death\n      } = d;\n\n      this.arrPositive.push({ date, total: positive });\n      this.arrHospitalized.push({ date, total: hospitalizedCurrently });\n      this.arrInIcu.push({ date, total: inIcuCurrently });\n      this.arrOnVentilators.push({ date, total: onVentilatorCurrently });\n      this.arrRecovered.push({ date, total: recovered });\n      this.arrDeaths.push({ date, total: death });\n    });\n  }\n};\n</script>\n\n<style>\n</style>\n"],"sourceRoot":""}]}